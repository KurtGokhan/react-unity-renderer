{
  "name": "@reactunity/renderer",
  "version": "0.0.14",
  "description": "React renderer for Unity3D",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -d",
    "watch": "tsc -d --watch",
    "build:clean": "rimraf dist",

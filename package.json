    "react",
    "react-renderer",
    "unity",
    "unity3d"
  ],
  "dependencies": {
    "react-reconciler": "^0.26.1"
  },
  "peerDependencies": {
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-reconciler": "^0.18.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }

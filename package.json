{
  "name": "react-unity-renderer",
  "version": "0.0.10",
  "description": "React renderer for Unity3D",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -d",
    "watch": "tsc -d --watch",
    "build:clean": "rimraf dist",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "prepare": "npm run build:clean && npm run build",
    "pack": "npm pack"
  },
  "author": "Gokhan Kurt <krtgkn@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KurtGokhan/react-unity-renderer"
  },
  "keywords": [
    "react",
    "react-renderer",
    "unity",
    "unity3d"
  ],
  "dependencies": {
    "react-reconciler": "^0.25.1"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-reconciler": "^0.18.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
